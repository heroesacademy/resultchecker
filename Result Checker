│   ├── dashboard.html          # Admin console
│   ├── members.html            # Member management
│   └── content.html            # Content management
├── assets/
│   ├── css/                    # All stylesheets
│   ├── js/                     # All JavaScript
│   ├── images/                 # All images
│   └── fonts/                  # Custom fonts
├── data/                       # JSON data files
│   ├── members.json            # Member database
│   ├── news.json               # News articles
│   └── events.json             # Event data
└── server/                     # Backend API (Node.js/PHP)
    ├── api/                    # API endpoints
